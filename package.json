{
  "name": "generic-route-decorators",
  "version": "0.2.1",
  "description": "ES7 decorators that simplify route creation, framework independent",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:webpack": "rimraf dist && webpack --config webpack.config.js",
    "package": "cpy package.json README.md LICENSE dist/",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asdftd/route-decorators-api"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "keywords": [
    "es6",
    "es7",
    "es2015",
    "es2016",
    "decorators",
    "koa",
    "express",
    "route",
    "routing"
  ],
  "author": "ASDFTF (https://github.com/asdftd)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asdftd/route-decorators-api/issues"
  },
  "homepage": "https://github.com/asdftd/route-decorators-api#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/jest": "^22.2.3",
    "awesome-typescript-loader": "^5.2.0",
    "chai": "^3.4.1",
    "cpy-cli": "^1.0.1",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
